  "dependencies": {
    "@stefanprobst/remark-shiki": "0.1.2",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "dayjs": "1.11.3",
    "disqus-react": "1.1.3",
    "github-markdown-css": "4.0.0",
    "gray-matter": "4.0.3",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reg-suit": "^0.12.1",
    "remark": "13.0.0",
    "remark-external-links": "8.0.0",
    "remark-gfm": "1.0.0",
    "remark-html": "13.0.2",
    "remark-prism": "1.3.6",
    "storycap": "^3.1.9",
    "typescript": "4.7.4"
