{
  "name": "yurikago-next",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "sitemap": "next-sitemap --config sitemap.config.js",
    "export": "yarn build && next export && yarn sitemap",
    "lint:eslint": "eslint --ignore-path .gitignore .",
